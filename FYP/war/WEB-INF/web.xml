<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
        <servlet-name>dispatcherServlet</servlet-name>
        <servlet-class>
            org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcherServlet</servlet-name>
        <url-pattern>/*.do</url-pattern>
    </servlet-mapping>
	
	<servlet>
		<servlet-name>formBuilder</servlet-name>
		<servlet-class>edu.fyp.servlet.FormBuilder</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>formBuilder</servlet-name>
		<url-pattern>/formbuilder/formBuilder</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>showFormListServlet</servlet-name>
		<servlet-class>edu.fyp.servlet.ShowFormList</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>showFormListServlet</servlet-name>
		<url-pattern>/Client/showFormListServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>home</servlet-name>
		<jsp-file>/Client/home.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/Client/home</url-pattern>
	</servlet-mapping>	
		<servlet>
		<servlet-name>showClientFormList</servlet-name>
		<jsp-file>/Client/showClientFormList.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>showClientFormList</servlet-name>
		<url-pattern>/Client/showClientFormList</url-pattern>
	</servlet-mapping>	
			<servlet>
		<servlet-name>showClientFormServlet</servlet-name>
		<servlet-class>edu.fyp.servlet.ShowForm</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>showClientFormServlet</servlet-name>
		<url-pattern>/Client/showClientFormServlet</url-pattern>
	</servlet-mapping>
			<servlet>
		<servlet-name>showClientForm</servlet-name>
		<jsp-file>/Client/showClientForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>showClientForm</servlet-name>
		<url-pattern>/Client/showClientForm</url-pattern>
	</servlet-mapping>	
	
		<servlet>
		<servlet-name>addFormPath</servlet-name>
		<servlet-class>edu.fyp.servlet.AddFormPath</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>addFormPath</servlet-name>
		<url-pattern>/formbuilder/addFormPath</url-pattern>
	</servlet-mapping>
	
				<servlet>
		<servlet-name>applyApplication</servlet-name>
		<servlet-class>edu.fyp.servlet.ApplyApplication</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>applyApplication</servlet-name>
		<url-pattern>/Client/applyApplication</url-pattern>
	</servlet-mapping>
</web-app>